{
	"manifest_version": 3,
	"name": "Simple New Tab",
	"version": "1.0.0",
	"action": {
		"default_popup": "popup.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"background": {
		"service_worker": "src/background.ts"
	},
	"icons": {
		"128": "src/assets/icon.png"
	},
	"permissions": ["storage"]
}
